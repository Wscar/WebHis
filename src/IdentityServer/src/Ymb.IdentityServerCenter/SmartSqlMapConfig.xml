    <DbProvider Name="Mysql"/>
    <Write Name="WriteDB" ConnectionString="${ConnectionString}"/>
    <Read Name="ReadDB" Weight="100" ConnectionString="${ConnectionString}"></Read>
  </Database>
  <SmartSqlMaps>
    <SmartSqlMap Path="Ymb.IdentityServerDomain.Maps.Client.xml,Ymb.IdentityServerDomain" Type="Embedded" ></SmartSqlMap>  
  </SmartSqlMaps>
</SmartSqlMapConfig>
